   <Button fx:id="quitter" layoutX="192.0" layoutY="354.0" mnemonicParsing="false" onAction="#quit" text="Quitter" />
   <Label fx:id="erreur" layoutX="1.0" layoutY="313.0" prefHeight="41.0" prefWidth="361.0" textFill="#dd4a2d" />
   <Button fx:id="reserver" layoutX="105.0" layoutY="354.0" mnemonicParsing="false" onAction="#reserver" text="Reserver" />
   <TextField fx:id="commentfield" layoutX="25.0" layoutY="247.0" promptText="Commentaire" />
   <Button fx:id="commentaire" layoutX="226.0" layoutY="247.0" mnemonicParsing="false" onAction="#sendcomment" text="Envoyer" />

</AnchorPane>
